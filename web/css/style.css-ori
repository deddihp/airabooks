/**********Arrow Down **********/
.arrow-down {
	width:0;
	height:0;
	border-left:7px solid transparent;
	border-right:7px solid transparent;
	border-top:7px solid #e2e2e2;
	position:absolute;
	top:10px;
	margin: 0 0 0 10px;
}
.arrow-right {
	width:0;
	height:0;
	border-bottom:4px solid transparent;
	border-left:4px solid #e2e2e2;
	border-top:4px solid transparent;
	position:absolute;
	top:8px;
	margin: 0 0 0 10px;
}
