.wiki {
	//border:1px solid black;
	text-align:justify;
	padding:0 20px 0 20px;
}
.wiki p.wiki-title {
	font:normal 18px/18px arial, sans-serif;
	border-bottom:1px solid black;
	margin:10px 0 10px 0;
}

.wiki a.wiki_minititle {
	font:normal 14px/14px arial, sans-serif;
	margin:0px;
}
.wiki span.wiki_minititle {
	font:normal 13px/18px arial, sans-serif;
	margin:0 10px 0 0;
	//border:1px solid black;
}

.wiki p.wiki_minicontent {
	font-family:'Arial';
	font-size:13px;
	margin:5px 0 0 0;
	text-indent:15px;
}

.wiki div.synopsis {
	//border:1px solid black;
	float:left;
	width:100%;
	//550px;
}

span.synopsis {
	font:normal 13px/18px arial, sans-serif;
}

.wiki div.logo_n_info_parent {
	border:0px solid red;
	display:table;
	width:280px;
	float:right;
}
.wiki div.logo_n_info {
	border:1px solid #e2e2e2;
	width:280px;
	//height:100px;
	float:right;
	margin: 0 0 10px 10px;
	background-color:#fbfbfb;
	text-align:center;
	padding:0px;
}

.wiki div.logo_n_info p.title {
	//border:1px solid black;
	margin:5px;
	padding:2px;
	background-color:#b9cede;

	font-family:'Arial';
	font-size:14px;
	font-weight:bold;
}

.wiki div.logo_n_info table {
	//border:1px solid black;
	width:100%;
	text-align:left;
	font-family:'Arial';
	font-size:12px;
	font-weight:normal;
	padding:0 0 5px 10px;
}

.wiki div.logo_n_info table td.parameter {
	font-weight:bold;	
}


.wiki span {
	text-align:justify;
	font-family:'Arial';
	font-size:13px;
}

.wiki table.collection {
	font-family:'Arial';
	font-size:12px;
	border-collapse:collapse;
	width:100%;
	background-color:#fbfbfb;
	//border-color:#d2d2d2;
	//border:1px solid #d2d2d2;
}

.wiki table.collection tr.title {
	font-weight:bold;
	text-align:center;
	background-color:#f1f1f1;
}
